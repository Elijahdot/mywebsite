{
    "rewrite": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}